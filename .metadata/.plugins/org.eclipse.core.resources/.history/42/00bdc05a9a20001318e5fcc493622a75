package token;

public class Token {
	private TokenType name;
	private String lexeme;
	
	public Token(TokenType name, String lexeme){
		this.name = name;
		this.setLexeme(lexeme);
	}

	public String getLexeme() {
		return lexeme;
	}

	public void setLexeme(String lexeme) {
		this.lexeme = lexeme;
	}
	
	
}
